{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:react-hooks/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/no-extraneous-dependencies": "warn",
        "no-unused-vars": "warn",
        "react/no-array-index-key": "warn",
        "max-len": ["warn", {"code": 150, "ignoreComments": true}]
    }
}
